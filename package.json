{
	"name": "domino-game",
	"version": "2.0.4",
	"description": "Domino is a game multiplayer made in node socket.io, typescript, node.js and html5.",
	"main": "./._build/Server.js",
	"engines": {
		"node": "10.19.0"
	},
	"scripts": {
		"build": "eslint && tsc  && npm run minify",
		"serve": "nodemon --watch \"src/**/*\" -e ts --exec \"ts-node -r tsconfig-paths/register\" ./src/Server.ts",
		"start": "node ./._build/Server.js",
		"pm2:start": "pm2 start ./._build/Server.js --name dominio-felipefm32",
		"lint:ts:base": "tslint --project tsconfig.json --config tslint.json",
		"lint:ts": "npm run lint:ts:base \"src/**/*.ts\"",
		"minify": "jsmin -o public/js/outputs/output.js  public/js/outputs/Game.js  public/js/outputs/Modal.js  public/js/outputs/App.js ",
		"compile-front": "tsc -p public/tsconfig.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/eufelipemateus/NodeDomino.git"
	},
	"keywords": [
		"Domino",
		"Game",
		"multiplaer",
		"Online",
		"Express",
		"HTML5",
		"Typescript",
		"Jogos-online",
		"Learn",
		"socket.io",
		"realtime",
		"developers",
		"dev",
		"realtime",
		"Games",
		"jogo",
		"dev-pleno",
		"jogos",
		"geek",
		"dom-html",
		"dev-junior",
		"full-stack-developer"
	],
	"author": {
		"name": "Felipe Mateus",
		"email": "eu@felipemateus.com",
		"url": "https://eufelipemateus.com/"
	},
	"license": "CC",
	"dependencies": {
		"@types/socket.io-client": "^1.4.34",
		"@typescript-eslint/eslint-plugin": "^4.11.0",
		"dotenv": "8.2.0",
		"eslint": "^7.16.0",
		"eslint-plugin-import": "^2.22.1",
		"express": "^4.16.4",
		"express-session": "~1.15.1",
		"socket.io": "^3.0.4",
		"socket.io-client": "^3.0.4",
		"systemjs": "^6.8.2"
	},
	"devDependencies": {
		"@types/dotenv": "8.2.0",
		"@types/node": "^7.10.14",
		"@types/socket.io": "^2.1.12",
		"@typescript-eslint/parser": "^4.11.0",
		"eslint-plugin-jsdoc": "^30.7.9",
		"eslint-plugin-prefer-arrow": "^1.2.2",
		"jsmin": "^1.0.1",
		"lint-staged": "10.5.3",
		"nodemon": "^1.19.4",
		"ts-node": "9.1.1",
		"tsconfig-paths": "3.9.0",
		"typescript": "^4.1.3"
	},
	"bugs": {
		"url": "https://github.com/eufelipemateus/NodeDomino/issues"
	},
	"homepage": "https://domino.felipemateus.com"
}
